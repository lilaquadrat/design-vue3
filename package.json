{
  "name": "@lilaquadrat/design",
  "version": "3.0.0-beta.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist/app && vite build",
    "build-all": "rm -rf dist/ && pnpm build && pnpm build-ssr",
    "build-next-all": "rm -rf dist/ && pnpm build-next && pnpm build-next-ssr",
    "build-next": "rm -rf dist/app && vite build --mode next",
    "build-ssr": "rm -rf dist/server && vite build --ssr --mode production",
    "build-next-ssr": "rm -rf dist/server && vite build --ssr --mode next",
    "preview": "vite",
    "type-check": "vue-tsc --build --force",
    "cli": "node node_modules/@lilaquadrat/cli/lib/src/index.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:css": "stylelint \"src/**/*.{vue,less}\" --fix",
    "commit": "cz"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@lilaquadrat/interfaces": "^1.28.1",
    "@lilaquadrat/sdk": "^0.0.10",
    "@lilaquadrat/studio": "^9.14.3",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "highlight.js": "^11.11.1",
    "object-path": "^0.11.8",
    "pinia": "^3.0.2",
    "qrcode": "^1.5.4",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@lilaquadrat/cli": "^0.10.2",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.15.3",
    "@types/object-path": "^0.11.4",
    "@types/uuid": "^10.0.0",
    "@types/youtube": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.25.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.0.0",
    "less": "^4.3.0",
    "npm-run-all2": "^7.0.2",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier-eslint": "^16.4.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "serve": "^14.2.4",
    "stylelint": "^16.19.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.4",
    "vue-tsc": "^2.2.10"
  }
}
